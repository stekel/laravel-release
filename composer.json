{
    "name": "stekel/laravel-release",
    "description": "Helpful command for making deployment releases.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">7.1",
        "illuminate/console": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "stekel\\LaravelRelease\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "stekel\\LaravelRelease\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "stekel\\LaravelRelease\\LaravelReleaseServiceProvider"
            ],
            "aliases": {}
        }
    }
}
